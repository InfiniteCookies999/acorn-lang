
#if OS_GROUP_WINDOWS

native dllimport void* GetStdHandle(uint32 nStdHandle);

native dllimport int32 WriteFile(void*       hFile,
                                 const void* lpBuffer,
                                 uint32      nNumberOfBytesToWrite,
                                 uint32*     lpNumberOfBytesWritten,
                                 void*       lpOverlapped);

#elif OS_GROUP_UNIX

native isize write(int32 fd, const void* buf, usize count);

#endif

void write_val(int val) {

#if OS_GROUP_WINDOWS
    void* handle = GetStdHandle(-11);
    uint32 written;
    char c = as(char) val;
    WriteFile(handle, &c, 1, &written, null);
#elif OS_GROUP_UNIX
    char c = as(char) val;
    write(1, &c, 1);
#endif

}

void foo(int a, int b, int... s) {
    write_val(a);
    write_val(b);
    write_val(s[0]);
    write_val(s[1]);
    write_val(s[2]);
    write_val(s[3]);
    write_val(s[4]);
    write_val(s[5]);
    write_val(s[6]);
    write_val(s[7]);
}

void main() {
    foo(64, 63, 76, 101, 116, 115, 32, 103, 111, 33);
}